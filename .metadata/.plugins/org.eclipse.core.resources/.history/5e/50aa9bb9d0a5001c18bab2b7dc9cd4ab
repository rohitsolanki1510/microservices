server.port=8999
spring.application.name=role-ws
server.servlet.context-path=/role-ws

eureka.client.serviceUrl.defaulZone=http://localhost:8010/eureka
spring.devtools.restart.enabled=true
eureka.client.registerWithEureka=false
eureka.client.fetchRegistry=false

eureka.instance.instance-id=${spring.application.name}:${spring.application.instance_id:${random.value}}

spring.datasource.url=jdbc:mysql://localhost:3306/ms?useSSL=false&serverTimezone=UTC
spring.datasource.username=root
spring.datasource.password=root
